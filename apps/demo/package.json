{
  "name": "demo",
  "version": "1.0.0",
  "description": "Minimal demo for thebe",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "copy:static": "copyfiles -u 1 static/* dist",
    "copy:thebe": "mkdir -p dist/_static; cp -R ../../packages/thebe/lib dist/_static",
    "demo": "npm run copy:static; npm run copy:thebe",
    "start": "http-server ./dist"
  },
  "private": true,
  "author": "Steve Purves",
  "license": "MIT",
  "dependencies": {
    "copyfiles": "^2.4.1",
    "http-server": "^14.1.1"
  }
}
